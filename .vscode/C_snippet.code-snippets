  "Create a function template": {
    "prefix": "TEMPLATE_FUNCTION",
    "body": [
      "/*!****************************************************************************",
      "* @brief   ${1:brief}",
      "* @param   ${2:parameter}",
      "* @return  ${3:return}",
      "* @note    ${4:note}",
      "******************************************************************************/",
      "${5:return_parm} $TM_FILENAME_BASE$_${6:function_name}(${7:parameter} )",
      "{",
      "    ${8:function_body}",
      "}"
    ],
    "description": "Create a function template"
  },
