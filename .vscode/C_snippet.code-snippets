  "Create a template for a h file": {
    "prefix": "TEMPLATE_FILE_H",
    "body": [
      "/*!****************************************************************************",
      "*                          EPAM Systems Kft.                                  *",
      "*                      H-1083 Budapest, HUNGARY                               *",
      "*                                                                             *",
      "* (c) Copyright, EPAM Systems, Budapest, HU                                   *",
      "* All rights reserved.                                                        *",
      "*******************************************************************************",
      "* @file    $TM_FILENAME",
      "* @author  ${1:author}",
      "* @date    $CURRENT_YEAR.$CURRENT_MONTH.$CURRENT_DATE",
      "* @brief   ${2:brief}",
      "* @note    ${3:note} ",
      "******************************************************************************/",
      "#ifndef __$TM_FILENAME_BASE$_H",
      "#define __$TM_FILENAME_BASE$_H",
      "/******************************************************************************",
      "* Includes                                                                    *",
      "******************************************************************************/",
      "// Inclusion of standard headers, with <xxx.h> format",
      "",
      "// 3'th party headers (CMSIS, HAL, LL, OS)",
      "",
      "// Own project headers",
      "",
      "/******************************************************************************",
      "* Exported #define expression macros                                          *",
      "******************************************************************************/",
      "",
      "/******************************************************************************",
      "* Exported #define function macros                                            *",
      "******************************************************************************/",
      "",
      "/******************************************************************************",
      "* Exported enum                                                               *",
      "******************************************************************************/",
      "",
      "/******************************************************************************",
      "* Exported typedef                                                            *",
      "******************************************************************************/",
      "",
      "/******************************************************************************",
      "* Exported struct / union                                                     *",
      "******************************************************************************/",
      "",
      "/******************************************************************************",
      "* Exported enum                                                               *",
      "******************************************************************************/",
      "",
      "/******************************************************************************",
      "* Exported extern variables                                                   *",
      "  NOT RECOMENDED  !!!",
      "******************************************************************************/",
      "",
      "/******************************************************************************",
      "* Exported function prototype declarations                                    *",
      "******************************************************************************/",
      "",
      "#endif /* __$TM_FILENAME_BASE$_H */",
      "",
      "/******************************************************************************/",
      "/* END OF FILE                                                                */",
      "/******************************************************************************/",
      ""
    ],
    "description": "Create a template for a h file"
  }
